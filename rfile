pipeline {
    
    agent none 
    
    stages {
        
        stage('checkout'){
            steps {
                
           
	    checkout([$class: 'GitSCM', 
             branches: [[name: params.branchName]], 
			 doGenerateSubmoduleConfigurations: false, 
			 extensions: [], 
			 submoduleCfg: [],
			 userRemoteConfigs: [[url: 'https://github.com/manidhar1986/jenkins-test']]])
	}
        }
        
        stage('get docker'){
            
            agent  {
               dockerfile true 
               }
    
                        
            steps {
                sh 'npm --version'
            }
        }
    }
}
